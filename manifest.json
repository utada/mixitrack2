{
  "manifest_version": 2,
  "name": "MixiTrack2",
  "version": "1.0",
  "description": "MixiTrack2",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "http://mixi.jp/",
    "https://mixi.jp/"
  ],
  "content_scripts": [
    {
      "matches": ["http://*.mixi.jp/*", "https://*.mixi.jp/*" ],
      "js": ["scripts/jquery-2.0.3.min.js", "scripts/js-url/url.js", "scripts/mixi.js"]
    }
  ],
  "icons": {
     "16": "images/icon16.png",
     "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
